{
    "package": "srp.example",
    "include": [
        "tools/model_generator/v2/example/example_service_depl.json",
        "tools/model_generator/v2/example/example_did_depl.json"
    ],
    "component": {
        "ExampleApp": {
            "functional_groups": [
                "Startup",
                "Startup2",
                "Platform",
                "Running",
                "SafetyMode",
                "MW"
            ],
            "parms": "",
            "reporting_mode": "NoReporting",
            "logger": {
                "app_id": "EM--",
                "app_des": "EM server",
                "log_level": "kInfo",
                "log_mode": "kRemote|kConsole",
                "ctx": []
            },
            "provide": {
                "srp.example.ExampleDidDepl as ExampleDidPPort": {
                    "instanceID": 1,
                    "on": "diag"
                },
                "srp.example.ExampleService as ExampleServicePPort": {
                    "using": "srp.example.ExampleServiceDepl",
                    "instanceID": 1,
                    "on": "SomeIp",
                    "bind": {
                        "type": "UDP",
                        "port": 10002
                    }
                }
            },
            "require": {
                "srp.example.ExampleService as ExampleServiceRPort": {
                    "using": "srp.example.ExampleServiceDepl",
                    "instanceID": 1,
                    "on": "IPC"
                }
            }
        }
    }
}